{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "http://localhost:*"
        },
        {
          "url": "http://127.0.0.1:*"
        }
      ]
    },
    "opener:default"
  ]
}